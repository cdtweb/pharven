{
    "name": "axute/pharven",
    "description": "Pharven bundles your vendor directory into a PHP Archive (PHAR)",
    "version": "0.1.3",
    "license": "MIT",
    "authors": [
        {
            "name": "axute",
            "email": "axutes@gmail.com"
        }
    ],
    "keywords": [
        "composer",
        "phar"
    ],
    "homepage": "https://github.com/axute/pharven",
    "require": {
        "roave/security-advisories": "dev-master",
        "php": ">=7.1",
        "twig/twig": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Pharven\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pharven\\": "tests"
        }
    },
    "bin": [
        "bin/pharven"
    ]
}
